<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to t('.info'), admin_test_path(test) %></td>
  <td><%= link_to t('.edit'), edit_admin_test_path(test) %></td>
  <td>
    <%= link_to t('.remove'),
    admin_test_path(test),
    method: :delete,
    data:   { confirm: t('.confirm') } %>
  </td>
</tr>
