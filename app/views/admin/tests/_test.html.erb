<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= pluralize(questions_count(test), 'item') %></td>
  <td><%= link_to 'Info', admin_test_path(test) %></td>
  <td><%= link_to 'Edit', edit_admin_test_path(test) %></td>
  <td>
    <%= link_to 'Delete',
    admin_test_path(test),
    method: :delete,
    data:   { confirm: 'Are you sure?' } %>
  </td>
</tr>
